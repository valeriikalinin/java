package com.max.idea;
public class Main {
    public static void main (String[] args) {

        int[] a = new int[] {10, 20, 30, 40, 50}; //объявляем и инициализируем массив
        int b = a[0];
            a[0] = a[4]; //ещё указывал значение a.length-1 и тоже работает
                a[4] = b;

        System.out.println(a[0]);
        System.out.println(a[2]);

    }
}
